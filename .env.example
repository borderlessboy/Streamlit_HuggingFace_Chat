# HuggingFace API Settings
HUGGING_FACE_API_TOKEN=your_token_here
DEFAULT_MODEL=Qwen/Qwen2.5-Coder-32B-Instruct

# Cache Settings
CACHE_TTL=3600                # Cache time-to-live in seconds (default: 1 hour)
MAX_CACHE_SIZE=1000          # Maximum number of items in memory cache
MAX_CONTEXT_LENGTH=10        # Maximum number of messages to keep in conversation context
DEFAULT_TIMEOUT=30           # API request timeout in seconds
MAX_FILE_SIZE=5242880       # Maximum file size in bytes (default: 5MB)

# Redis Settings (optional)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=             # Optional: Redis password if authentication is enabled

# Logging Configuration
LOG_LEVEL=INFO             # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL

# Model Parameters
MODEL_TEMPERATURE=0.7      # Controls randomness (0.0 to 1.0)
MODEL_TOP_P=0.9           # Nucleus sampling parameter (0.0 to 1.0)
MODEL_MAX_TOKENS=1024     # Maximum number of tokens to generate
MODEL_REP_PENALTY=1.1     # Repetition penalty (1.0 to 2.0)

# Application Settings
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=localhost
STREAMLIT_THEME_BASE=light  # Options: light, dark
STREAMLIT_BROWSER_GATHER_USAGE_STATS=false 